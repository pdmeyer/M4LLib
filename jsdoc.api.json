{
  "tags": {
    "allowUnknownTags": true,
    "dictionaries": ["jsdoc"]
  },
  "source": {
    "include": ["lib/pdm.m4l.lib.js"],
    "includePattern": ".js$",
    "excludePattern": "(node_modules/|docs/|m4l-errors.js)"
  },
  "plugins": ["plugins/markdown"],
  "templates": {
    "cleverLinks": false,
    "monospaceLinks": false,
    "default": {
      "outputSourceFiles": false,
      "includeDate": false,
      "useLongnameInNav": true,
      "staticFiles": {
        "include": []
      }
    }
  },
  "opts": {
    "destination": "./docs/api",
    "recurse": false,
    "readme": "./README.md",
    "template": "node_modules/clean-jsdoc-theme",
    "theme_opts": {
      "default_theme": "dark",
      "title": "M4LLib API Reference",
      "homepageTitle": "M4LLib API Reference",
      "base_url": "/",
      "menu": [
        {
          "label": "Github repo",
          "href": "https://github.com/pdmeyer/M4LLib",
          "target": "_blank",
          "class": "menu-item",
          "id": "repository"
        }
      ],
      "search": true,
      "exclude_inherited": true,
      "sections": ["Classes"],
      "sort": true,
      "displayModuleHeader": false
    }
  },
  "markdown": {
    "parser": "gfm",
    "hardwrap": false,
    "idInHeadings": true
  }
}
