{
  "name": "m4l-lib",
  "version": "0.0.1",
  "description": "A utility library for Max for Live (M4L) operations, providing helpful functions for interacting with Ableton Live's API",
  "main": "lib/pdm.m4l.lib.js",
  "keywords": [
    "max-for-live",
    "ableton-live",
    "midi",
    "audio",
    "music-production",
    "live-api",
    "javascript"
  ],
  "author": "Philip Meyer",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/pdmeyer/M4LLib.git"
  },
  "bugs": {
    "url": "https://github.com/pdmeyer/M4LLib/issues"
  },
  "homepage": "https://github.com/pdmeyer/M4LLib#readme",
  "engines": {
    "node": ">=14.0.0"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "docs": "jsdoc lib/*.js -d docs/api -c jsdoc.json",
    "docs:html": "jsdoc lib/*.js -d docs/html -c jsdoc.json",
    "docs:api": "jsdoc lib/pdm.m4l.lib.js -d docs -c jsdoc.api.json",
    "docs:md": "jsdoc2md lib/pdm.m4l.lib.js > docs/API.md",
    "docs:clean": "rm -rf docs/html docs/api docs/API.md",
    "docs:generate": "npm run docs:clean && npm run docs:md && npm run docs:html",
    "lint": "echo \"Linting not yet configured\"",
    "build": "echo \"No build step required for M4L library\""
  },
  "devDependencies": {
    "clean-jsdoc-theme": "^4.2.14",
    "jsdoc": "^4.0.2",
    "jsdoc-to-markdown": "^8.0.0"
  },
  "files": [
    "lib/",
    "docs/",
    "examples/",
    "README.md",
    "LICENSE",
    "CHANGELOG.md"
  ]
}
